{
  "root": "apps/tenable-io/host",
  "sourceRoot": "apps/tenable-io/host/src",
  "projectType": "application",
  "targets": {
    "serve": {
        "executor": "@nrwl/workspace:run-commands",
        "options": {
          "commands": [
            "cd apps/tenable-io/host && npm run dev",
            "cd apps/tenable-io/application-1 && npm run dev",
            "cd apps/tenable-io/application-2 && npm run dev"
          ]
        }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tenable-io/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/tenable-io/host"],
      "options": {
        "jestConfig": "apps/tenable-io/host/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
