{
  "root": "apps/tio",
  "sourceRoot": "apps/tio/src",
  "projectType": "application",
  "targets": {
    "serve": {
        "executor": "@nrwl/workspace:run-commands",
        "options": {
          "commands": [
            "cd apps/tio/src/host && npm run dev",
            "cd apps/tio/src/application-1 && npm run dev",
            "cd apps/tio/src/application-2 && npm run dev",
            "cd libs/design-system/components && npm run dev",
            "cd libs/design-system/styles && npm run dev",
            "cd libs/tio/common && npm run dev"
          ]
        }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tio/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/tio"],
      "options": {
        "jestConfig": "apps/tio/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
